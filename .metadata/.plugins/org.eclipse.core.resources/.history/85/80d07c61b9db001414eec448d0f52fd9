package database.access.dataobject;

public class UserInfoTable extends DatabaseTable {			
	public UserInfoTable(){
		super();
		this.setTableName(UserInfoTableEnum.TABLE_NAME.asText());
	}	
	
	public void addUsername(String newUsername){ // *** Create abstraction for all objects (i.e, UserId)?	
		this.addAttribute(UserInfoTableEnum.USERNAME.asText(), newUsername);		
	}
	
	public void addPassword(String newUserPassword){		
		String newUserPassword = this.formatStringAttr(newUserPassword);
		this.addAttribute(UserInfoTableEnum.PASSWORD.asText(), newUserPassword);
	}
	
}
