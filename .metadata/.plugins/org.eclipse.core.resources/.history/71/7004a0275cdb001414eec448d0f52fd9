package database.access.dataobject;

public class UserInfo extends AbstractTable {			
	public UserInfo(){
		this.tableName = UserInfoEnum.TABLE_NAME.toString();
	}
	
	//This function below ONLY FOR TEST PURPOSES
	public void addUserId(){
		this.attributeList.put(UserInfoEnum.USER_ID.getAttrText(), "1");
	}
	
	public void addUsername(String newUsername){ // *** Create abstraction for all objects (i.e, UserId)?	
		this.attributeList.put(UserInfoEnum.USERNAME.getAttrText(), newUsername);		
	}
	
	public void addPassword(String newUserPassword){		
		this.attributeList.put(UserInfoEnum.USERNAME.getAttrText(), newUserPassword);
	}
}
