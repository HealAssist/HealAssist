package database.access.tableobject;

public class PatDemographicsTable extends DatabaseTable{		
	public static final String TABLE_NAME = PatDemographicsEnum.TABLE_NAME.toString();
	public static final String CHART_ID = PatDemographicsEnum.CHART_ID.toString();
	public static final String PATIENT_TITLE = PatDemographicsEnum.PATIENT_TITLE.toString();
	public static final String FIRSTNAME = PatDemographicsEnum.FIRSTNAME.toString();
	public static final String MIDDLENAME = PatDemographicsEnum.MIDDLENAME.toString();
	public static final String LASTNAME = PatDemographicsEnum.LASTNAME.toString();
	public static final String NAME_SUFFIX = PatDemographicsEnum.NAME_SUFFIX.toString();
	public static final String CHART_PHOTO = PatDemographicsEnum.CHART_PHOTO.toString();
	public static final String PRIMARY_PHONE = PatDemographicsEnum.PRIMARY_PHONE.toString();
	public static final String SECONDARY_PHONE = PatDemographicsEnum.SECONDARY_PHONE.toString();
	public static final String SMS_STATUS = PatDemographicsEnum.SMS_STATUS.toString();
	public static final String WORK_PHONE = PatDemographicsEnum.WORK_PHONE.toString();
	public static final String WORK_PHONE_EXTENSION = PatDemographicsEnum.WORK_PHONE_EXTENSION.toString();
	public static final String PRIMARY_EMAIL = PatDemographicsEnum.PRIMARY_EMAIL.toString();
	public static final String ALTERNATE_EMAIL = PatDemographicsEnum.ALTERNATE_EMAIL.toString();
	public static final String PREFERRED_COMMUNICATION = PatDemographicsEnum.PREFERRED_COMMUNICATION.toString();
	public static final String REMINDER_LANGUAGE = PatDemographicsEnum.REMINDER_LANGUAGE.toString();
	public static final String MEDICAL_HISTORY_CONSENT_STATUS = PatDemographicsEnum.MEDICAL_HISTORY_CONSENT_STATUS.toString();
	public static final String SSN = PatDemographicsEnum.SSN.toString();
	public static final String DATE_OF_BIRTH = PatDemographicsEnum.DATE_OF_BIRTH.toString();
	public static final String GENDER = PatDemographicsEnum.GENDER.toString();
	public static final String RACE = PatDemographicsEnum.RACE.toString();
	public static final String ETHNICITY = PatDemographicsEnum.ETHNICITY.toString();
	public static final String PREFERRED_LANGUAGE = PatDemographicsEnum.PREFERRED_LANGUAGE.toString();
	public static final String STUDENT_STATUS = PatDemographicsEnum.STUDENT_STATUS.toString();
	public static final String ORIGINATING_COUNTRY = PatDemographicsEnum.ORIGINATING_COUNTRY.toString();
	public static final String MERITAL_STATUS = PatDemographicsEnum.MERITAL_STATUS.toString();
	public static final String PREGNANCY_STATE = PatDemographicsEnum.PREGNANCY_STATE.toString();
	public static final String SMOKING_STATUS = PatDemographicsEnum.SMOKING_STATUS.toString();
	
	public PatDemographicsTable(){
		super();
		this.setTableName(TABLE_NAME);
	}			
	public PatDemographicsTable addChartId(String newId){		
		this.addNumAttr(CHART_ID, newId);
		return this;
	}
	public PatDemographicsTable addPatientTitle(String newTitle){		
		this.addNumAttr(PATIENT_TITLE, newTitle);
		return this;
	}
	public PatDemographicsTable addFirstName(String newFirstName){		
		this.addStrAttr(FIRSTNAME, newFirstName);
		return this;
	}
	public PatDemographicsTable addMiddleName(String newMiddleName){		
		this.addStrAttr(MIDDLENAME, newMiddleName);
		return this;
	}
	public PatDemographicsTable addLastName(String newLastName){		
		this.addStrAttr(LASTNAME, newLastName);
		return this;
	}
	public PatDemographicsTable addSuffix(String newSuffix){		
		this.addStrAttr(NAME_SUFFIX, newSuffix);
		return this;
	}
	public PatDemographicsTable addChartPhoto(String newChartPhoto){		
		this.addStrAttr(CHART_PHOTO, newChartPhoto); // *** Add addBlobAttr() to DatabaseTable
		return this;
	}
	public PatDemographicsTable addPrimaryPhoneNumber(String newPhoneNumber){		
		this.addNumAttr(PRIMARY_PHONE, newPhoneNumber);
		return this;
	}
	public PatDemographicsTable addSecondaryPhoneNumber(String newPhoneNumber){		
		this.addNumAttr(SECONDARY_PHONE, newPhoneNumber);
		return this;
	}
	public PatDemographicsTable addSmsStatus(String newSmsStatus){		
		this.addNumAttr(SMS_STATUS, newSmsStatus);
		return this;
	}
	public PatDemographicsTable addWorkPhoneNumber(String newPhoneNumber){		
		this.addNumAttr(WORK_PHONE, newPhoneNumber);
		return this;
	}
	public PatDemographicsTable addWorkPhoneExtension(String newExtension){		
		this.addNumAttr(WORK_PHONE_EXTENSION, newExtension);
		return this;
	}
	public PatDemographicsTable addPrimaryEmail(String newEmail){		
		this.addStrAttr(PRIMARY_EMAIL, newEmail);
		return this;
	}
	public PatDemographicsTable addAlternateEmail(String newEmail){		
		this.addStrAttr(ALTERNATE_EMAIL, newEmail);
		return this;
	}
	public PatDemographicsTable addPreferredCommunication(String newPrefComm){		
		this.addNumAttr(PREFERRED_COMMUNICATION, newPrefComm);
		return this;
	}
}
