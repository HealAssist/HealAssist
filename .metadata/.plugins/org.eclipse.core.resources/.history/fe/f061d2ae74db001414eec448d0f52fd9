package database.access.dataobject;

public abstract enum TableAttrEnum {
	TableAttrEnum(){
		this.setStrRepresentation("");		
	}
	
	protected String asText(){
		return this.stringRepresentation;
	}
	protected void setStrRepresentation(String newRep){
		this.stringRepresentation = newRep;
	}
	
	private String stringRepresentation;	
}
