package database.access.dataobject;

public class UserInfo extends AbstractTable {			
	public UserInfo(){
		this.tableName = UserInfoEnum.TABLE_NAME.toString();
	}
	
	public void addUsername(String newUsername){ // *** Create abstraction for all objects (i.e, UserId)?	
		this.attributeList.put(UserInfoEnum.USERNAME.toString(), newUsername);		
	}
	
	public void addPassword(String newUserPassword){		
		this.attributeList.put(UserInfoEnum.USERNAME.toString(), newUserPassword);
	}
}
