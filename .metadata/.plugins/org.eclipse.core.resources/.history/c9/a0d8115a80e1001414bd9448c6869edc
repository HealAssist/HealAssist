package database.access.tableobject;

public class PatDemographicsTable extends DatabaseTable{		
	public static final String TABLE_NAME = PatientInfoEnum.TABLE_NAME.toString();
	public static final String CHART_ID = PatientInfoEnum.CHART_ID.toString();
	public static final String PATIENT_TITLE = PatientInfoEnum.PATIENT_TITLE.toString();
	public static final String FIRSTNAME = PatientInfoEnum.FIRSTNAME.toString();
	public static final String MIDDLENAME = PatientInfoEnum.MIDDLENAME.toString();
	public static final String LASTNAME = PatientInfoEnum.LASTNAME.toString();
	public static final String NAME_SUFFIX = PatientInfoEnum.NAME_SUFFIX.toString();
	public static final String CHART_PHOTO = PatientInfoEnum.CHART_PHOTO.toString();
	public static final String PRIMARY_PHONE = PatientInfoEnum.PRIMARY_PHONE.toString();
	public static final String SECONDARY_PHONE = PatientInfoEnum.SECONDARY_PHONE.toString();
	public static final String SMS_STATUS = PatientInfoEnum.SMS_STATUS.toString();
	public static final String WORK_PHONE = PatientInfoEnum.WORK_PHONE.toString();
	public static final String WORK_PHONE_EXTENSION = PatientInfoEnum.WORK_PHONE_EXTENSION.toString();
	public static final String PRIMARY_EMAIL = PatientInfoEnum.PRIMARY_EMAIL.toString();
	public static final String ALTERNATE_EMAIL = PatientInfoEnum.ALTERNATE_EMAIL.toString();
	public static final String PREFERRED_COMMUNICATION = PatientInfoEnum.PREFERRED_COMMUNICATION.toString();
	public static final String REMINDER_LANGUAGE = PatientInfoEnum.REMINDER_LANGUAGE.toString();
	public static final String MEDICAL_HISTORY_CONSENT_STATUS = PatientInfoEnum.MEDICAL_HISTORY_CONSENT_STATUS.toString();
	public static final String SSN = PatientInfoEnum.SSN.toString();
	public static final String DATE_OF_BIRTH = PatientInfoEnum.DATE_OF_BIRTH.toString();
	public static final String GENDER = PatientInfoEnum.GENDER.toString();
	public static final String RACE = PatientInfoEnum.RACE.toString();
	public static final String ETHNICITY = PatientInfoEnum.ETHNICITY.toString();
	public static final String PREFERRED_LANGUAGE = PatientInfoEnum.PREFERRED_LANGUAGE.toString();
	public static final String STUDENT_STATUS = PatientInfoEnum.STUDENT_STATUS.toString();
	public static final String ORIGINATING_COUNTRY = PatientInfoEnum.ORIGINATING_COUNTRY.toString();
	public static final String MERITAL_STATUS = PatientInfoEnum.MERITAL_STATUS.toString();
	public static final String PREGNANCY_STATE = PatientInfoEnum.PREGNANCY_STATE.toString();
	public static final String SMOKING_STATUS = PatientInfoEnum.SMOKING_STATUS.toString();
	
	public PatDemographicsTable(){
		super();
		this.setTableName(TABLE_NAME);
	}			
	public PatientInfoTable addChartId(String newId){		
		this.addNumAttr(CHART_ID, newId);
		return this;
	}
	public PatientInfoTable addPatientTitle(String newTitle){		
		this.addNumAttr(PATIENT_TITLE, newTitle);
		return this;
	}
	public PatientInfoTable addFirstName(String newFirstName){		
		this.addStrAttr(FIRSTNAME, newFirstName);
		return this;
	}
	public PatientInfoTable addMiddleName(String newMiddleName){		
		this.addStrAttr(MIDDLENAME, newMiddleName);
		return this;
	}
	public PatientInfoTable addLastName(String newLastName){		
		this.addStrAttr(LASTNAME, newLastName);
		return this;
	}
	public PatientInfoTable addSuffix(String newSuffix){		
		this.addStrAttr(NAME_SUFFIX, newSuffix);
		return this;
	}
	public PatientInfoTable addChartPhoto(String newChartPhoto){		
		this.addStrAttr(CHART_PHOTO, newChartPhoto); // *** Add addBlobAttr() to DatabaseTable
		return this;
	}
	public PatientInfoTable addPrimaryPhoneNumber(String newPhoneNumber){		
		this.addNumAttr(PRIMARY_PHONE, newPhoneNumber);
		return this;
	}
	public PatientInfoTable addSecondaryPhoneNumber(String newPhoneNumber){		
		this.addNumAttr(SECONDARY_PHONE, newPhoneNumber);
		return this;
	}
	public PatientInfoTable addSmsStatus(String newSmsStatus){		
		this.addNumAttr(SMS_STATUS, newSmsStatus);
		return this;
	}
	public PatientInfoTable addWorkPhoneNumber(String newPhoneNumber){		
		this.addNumAttr(WORK_PHONE, newPhoneNumber);
		return this;
	}
	public PatientInfoTable addWorkPhoneExtension(String newExtension){		
		this.addNumAttr(WORK_PHONE_EXTENSION, newExtension);
		return this;
	}
	public PatientInfoTable addPrimaryEmail(String newEmail){		
		this.addStrAttr(PRIMARY_EMAIL, newEmail);
		return this;
	}
	public PatientInfoTable addAlternateEmail(String newEmail){		
		this.addStrAttr(ALTERNATE_EMAIL, newEmail);
		return this;
	}
	public PatientInfoTable addPreferredCommunication(String newPrefComm){		
		this.addNumAttr(PREFERRED_COMMUNICATION, newPrefComm);
		return this;
	}
	public PatientInfoTable addReminderLanguage(String newLanguage){		
		this.addNumAttr(REMINDER_LANGUAGE, newLanguage);
		return this;
	}
	public PatientInfoTable addMedicalHistoryConsentStatus(String newStatus){		
		this.addNumAttr(MEDICAL_HISTORY_CONSENT_STATUS, newStatus);
		return this;
	}
	public PatientInfoTable addSSN(String newSSN){		
		this.addNumAttr(SSN, newSSN);
		return this;
	}
	public PatientInfoTable addDateOfBirth(String newDOB){		
		this.addNumAttr(DATE_OF_BIRTH, newDOB); // *** Add addDateAttr() to parent DatabaseTable
		return this;
	}
	public PatientInfoTable addGender(String newGender){		
		this.addCharAttr(GENDER, newGender);
		return this;
	}
	public PatientInfoTable addRace(String newRace){		
		this.addStrAttr(RACE, newRace);
		return this;
	}
	public PatientInfoTable addEthnicity(String newEthnicity){		
		this.addStrAttr(ETHNICITY, newEthnicity);
		return this;
	}
	public PatientInfoTable addPreferredLanguage(String newPrefLanguage){		
		this.addNumAttr(PREFERRED_LANGUAGE, newPrefLanguage);
		return this;
	}
	public PatientInfoTable addStudentStatus(String newStudentStatus){		
		this.addNumAttr(STUDENT_STATUS, newStudentStatus);
		return this;
	}
	public PatientInfoTable addOriginatingCountry(String newCountry){		
		this.addCharAttr(ORIGINATING_COUNTRY, newCountry);
		return this;
	}
	public PatientInfoTable addMeritalStatus(String newMeritalStatus){		
		this.addNumAttr(MERITAL_STATUS, newMeritalStatus);
		return this;
	}
	public PatientInfoTable addPregnancyState(String newPregnancyState){		
		this.addNumAttr(PREGNANCY_STATE, newPregnancyState);
		return this;
	}
	public PatientInfoTable addSmokingStatus(String newStatus){		
		this.addNumAttr(SMOKING_STATUS, newStatus);
		return this;
	}
}
